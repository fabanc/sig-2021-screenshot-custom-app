{
  "map": {
    "conditions": ["renderer"]
  },
  "about": {
    "title": {
      "express": true,
      "defaultValue": ""
    },
    "splash": {
      "defaultValue": false,
      "express": true,
      "splashButtonPosition": {
        "defaultValue": "top-right"
      },
      "splashOnStart": {
        "defaultValue": true
      },
      "splashTitle": {
        "express": true,
        "defaultValue": "Splash title"
      },
      "splashContent": {
        "express": true,
        "defaultValue": "Splash content"
      },
      "splashButtonText": {
        "defaultValue": "Okay"
      }
    },
    "customHeader": {
      "defaultValue": false,
      "customHeaderHTML": {
        "defaultValue": ""
      }
    }
  },
  "themeLayout": {
    "theme": {
      "applySharedTheme": {
        "express": true,
        "defaultValue": true
      },
      "interactiveLegendPosition": {
        "express": true,
        "defaultValue": "bottom-left"
      },
      "customCSS": {
        "defaultValue": ""
      }
    }
  },
  "interactivity": {
    "exploreNavigate": {
      "explore": {
        "home": {
          "defaultValue": true,
          "homePosition": {
            "defaultValue": "top-left"
          }
        },
        "mapZoom": {
          "defaultValue": true,
          "mapZoomPosition": {
            "defaultValue": "top-left"
          }
        },
        "infoPanel": {
          "defaultValue": false,
          "infoPanelPosition": {
            "defaultValue": "top-left"
          }
        }
      }
    },
    "modify": {
      "modify": {
        "basemapToggle": {
          "defaultValue": false,
          "basemapTogglePosition": {
            "defaultValue": "bottom-right"
          },
          "nextBasemap": {
            "defaultValue": "topo"
          }
        },
        "layerList": {
          "defaultValue": false,
          "layerListPosition": {
            "defaultValue": "bottom-right"
          }
        }
      }
    },
    "interactiveLegend": {
      "interactiveLegend": {
        "filterMode": {
          "defaultValue": "featureFilter"
        },
        "muteOpacity": {
          "defaultValue": 30
        },
        "muteGrayScale": {
          "defaultValue": 70
        },
        "featureCount": {
          "express": true,
          "defaultValue": false
        },
        "updateExtent": {
          "express": true,
          "defaultValue": false
        }
      }
    },
    "share": {
      "screenshot": {
        "screenshot": {
          "express": false,
          "defaultValue": false,
          "screenshotPosition": {
            "defaultValue": "top-left"
          },
          "enableLegendOption": {
            "defaultValue": true
          },
          "enablePopupOption": {
            "defaultValue": false
          }
        }
      }
    },
    "search": {
      "search": {
        "search": {
          "express": true,
          "defaultValue": true,
          "searchOpenAtStart": {
            "defaultValue": false
          },
          "searchPosition": {
            "defaultValue": "top-right"
          },
          "searchConfiguration": {
            "express": true
          }
        }
      }
    },
    "customURLParams": {
      "customURLParams": {
        "customURLParamName": {
          "express": false
        },
        "customUrlParam": {
          "express": false,
          "config": {
            "layerSelectionMode": "single",
            "supportedLayerTypes": "*",
            "supportedGeometryTypes": "*",
            "supportsFieldSelection": true,
            "fieldSelectionMode": "single",
            "supportedFieldTypes": "*"
          }
        }
      }
    }
  }
}
